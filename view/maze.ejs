<div id="maze-rotateAlert" class="rotate-alert">
  <div>
    <span class="glyphicon glyphicon-retweet" aria-hidden="true" style="transform: rotate(-90deg);margin-bottom: 40px;font-size: 200px;"></span>
    <p> 화면의 가로를 길게 해주세요! </p>
  </div>
</div>

<!-- maze-notification for vertical version -->

<div id="maze-notification" class="noti-container">
  <div class="noti-header" data-msg="goal"> 목표 </div>
  <div class="noti-body">
    <!-- <div class="noti-char"> -->
      <img class="noti-char"/>
    <!-- </div> -->
    <div class="noti-guide-scrollHidden">
      <div class="noti-guide"></div>
    </div>
    <div class="noti-direct">
      <div class="noti-arrow noti-up" data-arrow="up"></div>
      <div class="noti-arrow noti-down" data-arrow="down"></div>
    </div>
  </div>
</div>

<!-- ===================================== -->

<div id="maze-container" class="<%= mazeType %>">
  <div class="sidebar text-center">
    <canvas id="display"></canvas>
    <div class="side_guide">
      <div class="goal-title" data-msg="goal">목표</div>
      <div class="goal">
        <div class="goal-imgs">
          <img class="goal-img" src=""/>
        </div>
        <div class="goal-div">
          <span class="goal-msg"></span>
        </div>
      </div>
      <div class="side_console">
      </div>
    </div>
    <div class="bottom-row">
      <div id="scoreBox" style="display:none">
        <div class="food"></div> × <span class="score">0</span>
      </div>
      <!--a id="showCode" href="#" class="btn btn-lg btn-outline">
        <span class="glyphicon glyphicon-edit" aria-hidden="true"></span> 코드 보기
      </a-->
      <a id="runCode" href="#" class="btn btn-lg btn-outline" style="display:none">
        <div class="start">
          <span class="glyphicon glyphicon-play" aria-hidden="true"></span> <span data-msg="start">시작</span>
        </div>
        <div class="reset" style="display:none">
          <span class="glyphicon glyphicon-refresh" aria-hidden="true"></span> <span data-msg="reset">처음상태로</span>
        </div>
      </a>
    </div>
  </div>
  <div class="workspace">
    <div id="blocklyDiv" style="height: 100%; width: 100%;"></div>
    <div id="virtualKeypad">
      <div class="key up" data-direct="u"></div>
      <div class="key down" data-direct="d"></div>
      <div class="key left" data-direct="l"></div>
      <div class="key right" data-direct="r"></div>
    </div>
  </div>
</div>

<div id="modal" class="modal">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close" tabindex="0"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title"><span data-msg="title">키즈씽킹</span></h4>
      </div>
      <div class="modal-body">
        <div class="modal-msg-box">
          <div class="modal-imgs">
            <img class="modal-character" src="" />
          </div>
          <div class="modal-div">
            <span class="modal-msg"></span>
          </div>
        </div>
        <video autoplay controls>
          <source src="" type="video/mp4">
          Your browser does not support HTML5 video.
        </video>
        <div class="modal-select">
          <div><span data-msg="select_character">캐릭터를 선택해 보세요.</span></div>
          <div style="text-align: center">
            <div>
              <img class="modal-character1" src="" style="display:inline-block;float:none" />
              <button type="button" class="character1-modal btn btn-default" tabindex="0" > <span data-msg="select">선택</span></button>
            </div>
            <div>
              <img class="modal-character2" src="" style="display:inline-block;float:none" />
              <button type="button" class="character2-modal btn btn-default" tabindex="0"> <span data-msg="select">선택</span></button>
            </div>
          </div>
        </div>
      </div>
      <!-- id="doFScreen" -->
      <div class="modal-footer">
        <div></div>
        <button type="button" class="confirm-no btn btn-default" data-dismiss="modal" tabindex="0"><span data-msg="confirm_no">아니오</span></button>
        <button type="button" class="confirm-yes btn btn-primary" data-dismiss="modal" tabindex="0"><span data-msg="confirm_yes">네</span></button>
        <button type="button" class="close-modal btn btn-default" data-dismiss="modal" tabindex="0"><span class="glyphicon glyphicon-remove-circle" aria-hidden="true"></span> <span data-msg="close">닫기</span></button>
        <button type="button" class="reset-maze btn btn-default" data-dismiss="modal" tabindex="0"><span class="glyphicon glyphicon-repeat" aria-hidden="true"></span> <span data-msg="retry">다시 해보기</span></button>
        <button type="button" class="go-next btn btn-primary" data-dismiss="modal" tabindex="0"><span data-msg="next_stage">다음 단계로 이동</span> <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span></button>
        <button type="button" class="dialogue-next btn btn-primary" tabindex="0"><span data-msg="next">다음</span> <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span></button>
      </div>
    </div><!-- /.modal-content -->
  </div><!-- /.modal-dialog -->
</div><!-- /.modal -->

